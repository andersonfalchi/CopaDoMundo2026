{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [ "/css/*", "/js/*", "/_framework/*", "/*.{css,js,json,ico,png,jpg,jpeg,gif,svg}" ]
  },
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": [ "anonymous" ]
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  },
  "globalHeaders": {
    "cache-control": "public, max-age=31536000, immutable"
  },
  "mimeTypes": {
    ".json": "application/json",
    ".wasm": "application/wasm"
  }
}
